  script(src='anna/puzzletastichopefullyworks/annasgame/TemplateData/UnityProgress.js')
  body.template
    p.header
      span Unity WebGL Player 
      | Cube Collector
    .template-wrap.clear
      canvas#canvas.emscripten(oncontextmenu='event.preventDefault()', height='600px', width='960px')
      br
      .logo
      .fullscreen
        img(src='anna/puzzletastichopefullyworks/annasgame/TemplateData/fullscreen.png', width='38', height='38', alt='Fullscreen', title='Fullscreen', onclick='SetFullscreen(1);')
      .title Cube Collector
    p.footer
      | « created with 
      a(href='http://unity3d.com/', title='Go to unity3d.com') Unity
      |  »
    script(type='text/javascript').
      var Module = {
      TOTAL_MEMORY: 268435456,
      errorhandler: null,			// arguments: err, url, line. This function must return 'true' if the error is handled, otherwise 'false'
      compatibilitycheck: null,
      dataUrl: "anna/puzzletastichopefullyworks/annasgame/Release/puzzletastichopefullyworks.data",
      codeUrl: "anna/puzzletastichopefullyworks/annasgame/Release/puzzletastichopefullyworks.js",
