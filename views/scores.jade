extends layout

block content
  link(rel='stylesheet', href='stylesheets/scores.css', media='screen', title='no title', charset='utf-8')
  body
    header
    table
      tr
        th User Name
        th Total Score
        th High Score
      each user in info
        tr
          td #{user.username}
          td #{user.total_score}
          td #{user.high_score}

    footer
